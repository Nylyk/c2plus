{
    "manifest_version": 3,
    "name": "c2plus",
    "description": "c2plus",
    "version": "0.1.0",

    "content_scripts": [
        {
            "matches": ["*://app.chitchat.gg/*"],
            "run_at": "document_end",
            "js": ["index.js"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["injection.js"],
            "matches": ["*://app.chitchat.gg/*"]
        }
    ]
}
